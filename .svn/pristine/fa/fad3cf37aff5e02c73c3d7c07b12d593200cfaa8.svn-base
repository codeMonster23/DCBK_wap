        <script>
        $('.p-tabNav li.toggle').click(function () {
            // $('.sidebar').hide();
            $(this).addClass('active').siblings().removeClass('active');
        });
        $('.p-tabNav li.category').click(function (e) {
            $(this).addClass('active').siblings().removeClass('active');
            $('.shadow1').toggle();
            $(this).find('.sidebar').toggle();
            e.stopPropagation();
        });
        $('.category .sidebar li').click(function (e) {
            var index = $(this).index();
            var obj = $(this).parent('ol').siblings('.menu').find('.item').eq(index);
            $(this).addClass('active').siblings().removeClass('active');
            obj.addClass('active').siblings().removeClass('active');
            e.stopPropagation();
        });
        $('.category .menu a').click(function (e) {
            e.stopPropagation();
        });
        $('.share').click(function (e) {
            $('.shadow').show();
            $('.mask').show();
            e.stopPropagation();
            $('body').on('touchmove',function  (e) {
                e.preventDefault()
            })
        });
       
        $('.mask li a').click(function () {
            $('.shadow').show();
            $('.mask').hide();
            $('.copyModal').show();
        });
        $('.copyModal .btn').click(function () {
            $('.shadow').hide();
            $('.copyModal').hide();
            $('body').off('touchmove');
        });
        $('.shadow').click(function () {
            $(this).hide();
            $('.mask').hide();
            $('.copyModal').hide();
            $('body').off('touchmove');
        });

        $('.shadow1').click(function () {
            $(this).hide();
            $('.sidebar').hide();
        });
