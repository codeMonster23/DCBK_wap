/**首页**/

html,body {
    height: 100%;
}
body{
    position: relative;
    height: 100%;
}
.header {
    position: relative;
    height: 0.98rem;
    padding: 0 0.2rem;
    background-color: #393939;
}

.header .dropdown {
    width: 1.56rem;
    height: 0.56rem;
    margin: 0.2rem 0.08rem 0 0;
    border-radius: 5px;
    background-color: #fff;
    font-size: 0.22rem;
}
.header .dropdown a {
    position: relative;
    width: 100%;
    height: 0.56rem;
    line-height: 0.56rem;
    padding: 0;
    padding-left: 0.12rem;
    background-color: #666;
    border-radius: 4px;
    color: #dcdcdc;
    text-align: left;
}
.header .dropdown-menu>li>a {
    background-color: transparent;
    color: #333;
}
.header .dropdown a .caret {
    border: none;
    position: absolute;
    top: 0.23rem;
    right: 0.12rem;
    width: 0.24rem;
    height: 0.14rem;
    background: url(../images/index/icon1.png) no-repeat;
    -webkit-background-size: 0.24rem 0.14rem;
    background-size: 0.24rem 0.14rem;
}
.header .input-group {
    width: 4.36rem;
    height: 0.56rem;
    margin-top: 0.2rem;
}
.header .input-group-addon {
    width: 0.62rem;
    height: 0.56rem;
    padding: 0;
    border: none;
    background: #666 url(../images/index/icon2.png) center no-repeat;
    -webkit-background-size: 0.29rem;
    background-size: 0.29rem;
}
.header .input-group .form-control {
    width: 3.74rem;
    height: 0.56rem;
    border: none;
    background-color: #666;
    color: #dcdcdc;
}
.navbar {
    height: 2.13rem;
    padding-top: 0.35rem;
    margin-bottom: 0.14rem;
    border-bottom: 1px solid #eae9e9;
    background-color: #fff;
}

.imgWrap1 li,.subnav a {
    float: left;
    width: 25%;
    text-align: center;
    font-size: 0.24rem;
}
.imgWrap1 li img {
    width: 1.02rem;
    height: 1.02rem;
    margin: 0 auto;
}
.module2 .module-bd li img {
    width: 1.28rem;
    height: 1.28rem;
    border-radius: 50%;
}
.navbar h4 {
    margin-top: 0.21rem;
    font-size: 0.24rem;
}
.module {
    margin-top: 0.14rem;
    border-top: 1px solid #eae9e9;
    border-bottom: 1px solid #eae9e9;
    background-color: #fff;
}
.module1 {
    padding-bottom: 0.46rem;
}
.module .module-hd {
    height: 0.86rem;
    border-bottom: 1px solid #ebebeb;
}
.module .module-hd .line {
    float: left;
    width: 0.06rem;
    height: 0.32rem;
    background-color: #ff9934;
    margin: 0.3rem 0.12rem 0 0.15rem;
}
.module .module-hd .title {
    float: left;
    height: 0.86rem;
    line-height: 0.86rem;
    border-bottom: 1px solid #ef7b2d;
    font-size: 0.32rem;
}
.module2 .module-hd .left {
    width: 1.89rem;
    border-right: 1px solid #ebebeb;
}
.module2 .module-hd .right {
    width: 4.5rem;
    height: 0.86rem;
    line-height: 0.86rem;
    padding-left: 0.24rem;
    /* padding-right: 0.27rem; */
    font-size: 0.24rem;
    color: #ef7b2d;
}
.module .module-hd .more {
    padding-right: 0.27rem;
    line-height: 0.86rem;
    font-size: 0.24rem;
    /* color:#999; */
}
.imgWrap2 li {
    float: left;
    width: 50%;
}
.module .module-bd h4 {
    margin-top: 0.17rem;
    font-size: 0.24rem;
    color: #666;
}
.module ul {
    padding-top: 0.3rem;
}
.module1 ul {
    padding-left: 0.15rem;
}
.module2 ul {
    padding: 0.52rem 0 0.59rem 0;
}
.module1 li {
    width: 2.97rem;
    margin-right: 0.15rem;
}
.module1 img {
    width: 2.97rem;
    height: 2.18rem;
}
.module2 .module-bd h4 {
    margin-top: 0.29rem;
}
.module3 ul {
    padding-left: 0.2rem;
    border-bottom: 1px solid #ebebeb;
}
.module3 li {
    width: 2.9rem;
    margin: 0 0.2rem 0.32rem 0;
}
.module3 img {
    width: 2.9rem;
    height: 2.13rem;
}
.module3 .module-bd h4 {
    width: 2.9rem;
    margin-top: 0.26rem;
    -ms-text-overflow: ellipsis;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
.module3 .module-bd p {
    margin-top: 0.12rem;
    font-size: 0.2rem;
    color: #999;
}
.loading {
    display: block;
    height: 0.9rem;
    line-height: 0.9rem;
    border-bottom: 1px solid #f0f0f0;
    text-align: center;
    font-size: 0.24rem;
    color: #ff9934;
}
.loading.grey {
    color: #999;
}
.loading img {
    display: inline-block;
    vertical-align: middle;
    width: 0.3rem;
    height: 0.3rem;
}
.loading span {
    display: inline-block;
    line-height: 0.3rem;
    margin: 0 0.12rem;
}
/**系列丛书**/
header {
    position: relative;
    height: 0.86rem;
    line-height: 0.86rem;
    background: #333;
    -webkit-background-size: 0.16rem 0.27rem;
    background-size: 0.16rem 0.27rem;
    font-size: 0.28rem;
    text-align: center;
    color: #fff;
}
.seriesBook .con-item {
    padding: 0.1rem;
    margin-top: 0.2rem;
    box-shadow: 1px 1px 15px rgba(0, 0, 0, 0.15);
    background-color: #fff;
}
.seriesBook .con-item.last {
    margin-bottom: 0.25rem;
}
.seriesBook .caption {
    padding: 0.35rem 0.1rem 0.18rem;
    font-size: 0.24rem;
    color: #666;
}
.seriesBook .caption .badge {
    float: left;
    width: 0.66rem;
    height: 0.32rem;
    line-height: 0.32rem;
    margin: 0.08rem 0.15rem 0 0;
    padding: 0;
    border-radius: 0.33rem;
    background-color: #ffc266;
    text-align: center;
    color: #fff;
}
.seriesBook .caption p {
    float: left;
    width: 5.19rem;
    line-height: 0.48rem;
}
.bookDetail .searchInput {
    margin: 0.18rem auto;
}
.bookDetail .con-item {
    margin-top: 0;
    box-shadow: none;
    border-bottom: 1px solid #e1e1e1;
    background-color: #fff;
    padding-bottom: 0.2rem;
}
.bookDetail .caption {
    position: relative;
    padding: 0.35rem 0 0.18rem;
    color: #333;
}
.bookDetail .caption p {
    width: 100%;
}
.bookDetail .caption a.upfold,.bookDetail .caption a.fold {
    position: absolute;
    right: 0.1em;
    bottom: -0.05rem;
}
.bookDetail .caption a.upfold {
    background: url(../images/index/upfold.png) right center no-repeat;
    background-size: 0.17rem 0.1rem;
    padding-right: 0.22rem;
    margin-right: 0.2rem;
    font-size: 0.2rem;
    color: #3898d5;
}
.bookDetail .caption a.fold {
    background-image: url(../images/index/fold.png);
}
.bookDetail .s-header {
    height: 0.76rem;
    padding-left: 0.65rem;
    border-bottom: 1px solid #dfdede;
    background: #fff url(../images/index/icon4.png) 0.22rem center no-repeat;
    -webkit-background-size: 0.3rem 0.2rem;
    background-size: 0.3rem 0.2rem;
    font: bold 0.32rem/0.76rem "微软雅黑";
    color: #666;
}
.bookDetail .s-tab .tabNav {
    position: relative;
    background-color: #fff3eb;
    border-bottom: 1px solid #e1e1e1;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1);
}
.bookDetail .tabNav .overflow {
    float: left;
    width: 5.64rem;
    height: 0.76rem;
    line-height: 0.76rem;
}
.bookDetail .tabNav .overflow .toggle {
    display: block;
    padding-left: 0.22rem;
    font-size: 0.24rem;
    color: #999;
}
.bookDetail .tabNav .container {
    position: absolute;
    top: 0.76rem;
    width: 6.4rem;
    padding-top: 0.27rem;
    padding-bottom: 0.5rem;
    background-color: #e8ddd5;
    display: none;
}
.bookDetail .tabNav .container li {
    float: left;
    height: 0.76rem;
    line-height: 0.76rem;
    padding: 0 0.18rem;
}
.bookDetail .tabNav .container li a {
    display: block;
    height: 0.76rem;
    font-size: 0.24rem;
    color: #333;
}
.bookDetail .tabNav .overflow .slidenav {
    width: 5.64rem;
    height: 0.76rem;
    line-height: 0.76rem;
}
.bookDetail .slidenav a {
    color: #333;
}
.bookDetail .slidenav a.active {
    color: #f57828;
}
.bookDetail .tabNav .container li.cur {
    color: #f57828;
    border-bottom: 4px solid #f57828;
}
.bookDetail .tabNav .dropdown-toggle {
    position: absolute;
    right: 0;
    top: 0;
    width: 0.76rem;
    height: 0.76rem;
    border-left: 1px solid #dfdede;
    background: #fff url(../images/index/icon5.png) center no-repeat;
    -webkit-background-size: 0.27rem 0.15rem;
    background-size: 0.27rem 0.15rem;
}
.bookDetail .tabNav .dropdown-toggle.active {
    background-image: url(../images/index/icon5-b.png);
}
.bookDetail .p-tabNav li.nav-item {
    width: 50%;
}
.p-tabContent .item {
    margin-top: 0.25rem;
    padding-left: 0.18rem;
}
.p-tabContent .item h4 {
    height: 0.26rem;
    padding-left: 0.14rem;
    border-left: 6px solid #f57828;
    font: bold 0.28rem/0.26rem "微软雅黑";
}
.p-tabContent .item ul {
    overflow: hidden;
}
.p-tabContent .item li {
    float: left;
    width: 2.9rem;
    height: 3.3rem;
    border: 1px solid #e5e5e5;
    margin: 0.17rem 0.17rem 0 0;
    background-color: #fff;
    box-shadow: 1px 1px 1px rgba(0, 0, 0, 0.1);
}
.p-tabContent .item li .pic {
    width: 2.88rem;
    height: 2.1rem;
    border-bottom: 1px solid #e5e5e5;
    overflow: hidden;
}
.p-tabContent .item li .pic img {
    width: 2.88rem;
    height: 2.1rem;
}
.p-tabContent .item li .title {
    display: block;
    padding-left: 0.15rem;
    margin: 0.25rem 0 0.18rem;
    color: #333;
    font-size: 0.24rem;
}
.p-tabContent .item li p {
    padding: 0 0.15rem;
    font-size: 0.2rem;
    color: #a0a0a0;
    overflow: hidden;
}
.p-tabContent .item li .num {
    color: #787878;
}
.p-tabContent .item li .num span {
    margin-left: 0.2rem;
    background: url(../images/index/icon6.png) left center no-repeat;
    background-size: 0.26rem 0.15rem;
}
.p-tabContent .item li .num span.num1 {
    padding-left: 0.3rem;
    margin-left: 0;
}
.p-tabContent .item li span.num2 {
    padding-left: 0.23rem;
    background-image: url(../images/index/icon7.png);
    background-size: 0.19rem 0.18rem;
}
/**讲坛**/
header .search {
    position: absolute;
    right: 0;
    top: 0;
    width: 0.29rem;
    height: 0.29rem;
    margin-top: 0.34rem;
    margin-right: 0.25rem;
}
.p-tabNav {
    position: relative;
    background-color: #fff;
    border-bottom: 1px solid #e9e9e9;
}
.p-tabNav li.nav-item {
    float: left;
    width: 33.3%;
    padding: 0.25rem 0;
}

.p-tabNav li a {
    display: block;
    line-height: 0.36rem;
    border-right: 1px solid #dcdcdc;
    font-size: 0.28rem;
    text-align: center;
}

.p-tabNav li:last-child a {
    border-right: none;
}

.p-tabNav li span,
.p-tabNav li i {
    display: inline-block;
}

.p-tabNav li i {
    width: 0.2rem;
    height: 0.11rem;
    background: url(../images/index/icon5.png) no-repeat;
    -webkit-background-size: 0.2rem 0.11rem;
    background-size: 0.2rem 0.11rem;
    margin-left: 0.18rem;
    vertical-align: middle;
}

.p-tabNav li.r-category i {
    width: 0.28rem;
    height: 0.18rem;
    margin-right: 0.15rem;
    background-image: url(../images/index/icon4.png);
    background-size: 0.28rem 0.18rem;
}

.p-tabNav li.active a {
    color: #f57828;
}

.p-tabNav li.active i {
    background-image: url(../images/index/icon5-a.png);
}

/* .p-tabNav li.category.active {
    border-bottom: 2px solid #f57828;
} */

.p-tabNav li.r-category.active i {
    background-image: url(../images/index/icon4-a.png);
}

.sidebar {
    position: absolute;
    left: 0;
    top: 0.88rem;
    width: 6.4rem;
    height: 7rem;
    border: 1px solid #ffc399;
    z-index: 9999;
    display: none;
}

.bookDetail .sidebar {
    display: block;
    top: 0;
}

.editor .p-tabNav li.category .sidebar {
    height: 4.24rem;
}

.category .sidebar ol {
    float: left;
    width: 2.51rem;
    height: 100%;
    background-color: #ffeee3;
    overflow-y: auto;
}

.category .sidebar li {
    /* width: 2.55rem; */
    height: 0.7rem;
    border: 1px solid #f5dfd1;
    border-left: none;
    border-right-color: #d98d56;
    margin-top: -1px;
}

.category .sidebar li a {
    display: block;
    line-height: 0.7rem;
    padding-left: 0.15rem;
    text-align: left;
    font-size: 0.28rem;
    border: none;
    color: #333;
}

.category .sidebar li.active {
    background-color: #fff;
    /* 	position: relative;
	right: -1px; */
    border-color: #da9b6e;
}

.category .sidebar li.active a {
    /* border-left: 5px solid #e5430a; */
    color: #ef7009;
    position: relative;
    right: -1px;
    background-color: #fff;
    border-left: 5px solid #e5430a;
    border-bottom: 1px solid #da9b6e;
}

.category .menu {
    float: left;
    width: 3.79rem;
    height: 100%;
    background-color: #fff;
}

.category .menu .item {
    display: none;
    width: 100%;
    height: 100%;
    background-color: #fff;
    overflow-y: auto;
    border-left: 1px solid #d98d56;
    margin-left: -1px;
}

.category .menu .item.active {
    display: block;
}

.category .menu a {
    display: block;
    margin: 0 0.12rem;
    height: 0.7rem;
    line-height: 0.7rem;
    border: none;
    border-bottom: 1px solid #f6e6db;
    padding-left: 0.15rem;
    text-align: left;
    font-size: 0.28rem;
    color: #ef7009;
    margin-top: -1px;
}

.tab .tabContent .content {
    display: none;
}

.tab .tabContent .content.cur {
    display: block;
}


/**知网书**/

.p-tabNav1 li.toggle i {
    width: 0.18rem;
    height: 0.26rem;
    background-image: url(../images/index/icon15.png);
    background-size: 0.19rem 0.26rem;
}

.p-tabNav li.toggle.active {
    border-bottom: 2px solid #f57828;
}

.p-tabNav1 li.toggle.active i {
    background-image: url(../images/index/icon15-a.png);
}

.p-tabContent1 {
    margin-top: 0.15rem;
}

.p-tabContent1 .item {
    padding-top: 0.5rem;
    padding-left: 0.2rem;
    background-color: #fff;
}

.p-tabContent1 .item li {
    width: 2.88rem;
    margin: 0 0.2rem 0.35rem 0;
}

.showMore1 {
    background: none;
}


/**搜索**/

.searchWrap .dropdown {
    width: 1.42rem;
}

.searchWrap .input-group {
    width: 3.8rem;
    border-radius: 3px;
}

.searchWrap .input-group .form-control {
    border-radius: 3px;
    background-color: #666;
    color: #fff;
}

.searchWrap .cancel {
    float: left;
    line-height: 0.56rem;
    margin-top: 0.2rem;
    margin-left: 0.15rem;
    font-size: 0.28rem;
    color: #fff;
}

.blankWrap {
    padding-top: 3.62rem;
    background: url(../images/index/icon16.png) center 1.5rem no-repeat;
    -webkit-background-size: 1.44rem;
    background-size: 1.44rem;
    color: #333;
    text-align: center;
}

.searchPage .blankWrap h2 {
    font-size: 0.32rem;
}

.searchPage .blankWrap p {
    margin-top: 0.2rem;
    font-size: 0.24rem;
    color: #f57828;
}

.searchPage .hint {
    padding: 0.38rem 0 0.27rem 0.34rem;
    box-shadow: 1px 1px 1px rgba(0, 0, 0, 0.1);
}

.searchPage .hint a {
    float: left;
    line-height: 0.48rem;
    padding: 0 0.2rem;
    margin: 0 0.15rem 0.15rem 0;
    border: 1px solid #dcdcdc;
    font-size: 0.24rem;
    color: #666;
}

.searchPage .history {
    font-size: 0.24rem;
    color: #999;
}

.searchPage .history h2 {
    height: 0.68rem;
    line-height: 0.68rem;
    padding-left: 0.36rem;
    background-color: #f5f5f5;
    font-size: 0.28rem;
    color: #333;
    box-shadow: 1px 1px 3px rgba(0, 0, 0, 0.1);
}

.searchPage .history ul {
    padding: 0.14rem 0 0.1rem 0.36rem;
    border-bottom: 1px solid #e5e5e5;
}

.searchPage .history li {
    line-height: 0.6rem;
    padding-left: 0.6rem;
    background: url(../images/index/icon17.png) left center no-repeat;
    -webkit-background-size: 0.29rem;
    background-size: 0.29rem;
}

.searchPage .history .empty {
    margin-top: 0.3rem;
    text-align: center;
}

.media-list {
    background-color: #fff;
    box-shadow: 0 0 1px rgba(0, 0, 0, 0.15);
}

.media-list1 {
    margin-bottom: 0.2rem;
}

.media {
    position: relative;
    margin-top: 0;
    padding: 0.2rem;
    border-bottom: 1px solid #e5e5e5;
}

.media-left {
    padding-right: 0.2rem;
}

.media .media-object {
    width: 1.65rem;
    height: 1.65rem;
    border-radius: 50%;
}

.media .media-heading {
    margin-top: 0.05rem;
    margin-bottom: 0.2rem;
    font-size: 0.3rem;
    color: #333;
}

.media-body p {
    margin-bottom: 0.14rem;
    font-size: 0.24rem;
    color: #8c8c8c;
}

.media-list1 .media-body p {
    width: 4.13rem;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.media-body .btn {
    position: absolute;
    right: 0.2rem;
    top: 0.34rem;
    padding: 0;
    width: 1.1rem;
    height: 0.44rem;
    line-height: 0.44rem;
    border: 1px solid #c8c8c8;
    font-size: 0.24rem;
    color: #f57828;
}

.media-body .btn1 {
    width: 1.3rem;
    padding-left: 0.43rem;
    background: url(../images/index/icon19.png) 0.14rem center no-repeat;
    -webkit-background-size: 0.21rem 0.15rem;
    background-size: 0.21rem 0.15rem;
    color: #787878;
}

.media-body .badge {
    float: left;
    padding: 0 0.13rem;
    height: 0.32rem;
    line-height: 0.32rem;
    margin-right: 0.05rem;
    margin-bottom: 0.15rem;
    border-radius: 3px;
    font-size: 0.2rem;
    color: #fff;
    white-space: normal;
}

.badge.blue {
    background-color: #5db8e7;
}

.badge.orange {
    background-color: #f4a673;
}

.badge.green {
    background-color: #76d3bc;
}

.badge.purple {
    background-color: #9f9eec;
}

.media-list .more {
    line-height: 0.9rem;
    font-size: 0.24rem;
    color: #666;
    text-align: center;
}

.media-list .more i,
.media-list .more span {
    display: inline-block;
    vertical-align: middle;
}

.media-list .more i {
    width: 0.13rem;
    height: 0.22rem;
    background: url(../images/index/icon18.png) no-repeat;
    -webkit-background-size: 0.13rem 0.22rem;
    background-size: 0.13rem 0.22rem;
    margin-left: 0.12rem;
}

.media-list2 .media,.media-list3 .media {
    padding: 0.25rem 0.2rem;
    border-bottom-style: dashed;
}

.media-list2 .media.first,.media-list3 .media.first {
    border-top: none;
}

.media-list2 .media-object,.media-list3 .media-object {
    width: 2.4rem;
    height: 1.75rem;
    border: 1px solid #d2d2d2;
    border-radius: 0;
}

.media-list2 .media-body p, .media-list3 .media-body p{
    width: 3.42rem;
}

.searchPage .tabnav{
    height: 0.85rem;
    border-top: 1px solid #e5e5e5;
    background: #fff url("../images/index/line.jpg") no-repeat center;
    text-align: center;
}
.searchPage .tabnav a{
    display: inline-block;
    width: 1rem;
    height: 0.84rem;
    line-height: 0.84rem;
    margin: 0 0.27rem;
    font-size: 0.3rem;
    font-family: "Microsoft Yahei";
    text-align: center;
}
.searchPage .tabnav a.cur{
    height: 0.81rem;
